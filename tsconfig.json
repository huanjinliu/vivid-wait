{
  "compilerOptions": {
    "target": "ES2015",
    "module": "umd",
    "declaration": true,
    "esModuleInterop": true,
    "outDir": "./types",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "skipLibCheck": true
  }
}